        <section class="main_section">
            <img class="main_section__img" src="/icons/register-image.png"/>
            <section class="form_section">
                <h1 class="welcome_header"><b>Welcome to GSN</b></h1>
                form(method='POST', action='/users/register')
                    #form-group
                        <section class="form-section">
                            <img class="input-icon" src="/icons/icon-username.png"/>
                            input.form-control(class='input_username' name='name' type='text' placeholder='Name')
                            <div class="separate-green-line"/>
                        </section>
                    #form-group
                        <img class="input-icon" src="/icons/icon-email.png"/>
                        input.form-control(class='input_email' name='email' type='text' placeholder='Email')
                        <div class="separate-green-line"/>
                    #form-group
                        <img class="input-icon" src="/icons/icon-username.png"/>
                        input.form-control(class='input_username' name='username' type='text' placeholder='Username')
                        <div class="separate-green-line"/>
                    #form-group
                        <img class="input-icon" src="/icons/icon-password.png"/>
                        input.form-control(class='input_password' name='password' type='password' placeholder='Password')
                        <div class="separate-green-line"/>
                    #form-group
                        <img class="input-icon" src="/icons/icon-password.png"/>
                        input.form-control(class='input_password' name='password2' type='password' placeholder='Repeat Password')
                        <div class="separate-green-line"/>
                    br
                    <section class="button_section">
                        input.btn.btn-primary(class="btn-green" type='submit', value='Sign Up')
                        <button class="btn_login"><a href="/users/login">Log In</a></button>
                    </section>
                    <section class="terms_and_conditions_section">
                        <h1 class="terms_and_conditions__header">By signing up you agree to GSN's</h1>
                        <h1 class="terms_and_conditions__header"><a>Terms and conditions</a> & <a>Privacy Policy</a></h1>
                    </section>
            </section>

        </section>